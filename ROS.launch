<launch>

    <!-- HARDWARE -->
    <node pkg="rosserial_server" type="serial_node" name="rosserial_server_pot">
        <rosparam>
      port: /dev/ttyUSB1
      require:
        publishers: [ cmd_vel ]
        subscribers: [ power/status/pwm/right, power/status/pwm/left, power/status/speed/angular/right, power/status/speed/angular/left,power/status/speed/rpm/right,power/status/speed/rpm/left,power/status/distance/ticks/left,power/status/distance/ticks/right,power/status/seted/speed/rpm/left,power/status/seted/speed/rpm/right,power/status/debug/controler/rpm,power/status/debug/pwm ]
        </rosparam>
    </node>

     <node pkg="rosserial_server" type="serial_node" name="rosserial_server_sens">
        <rosparam>
      port: /dev/ttyUSB0
      require:
        publishers: [ ,cmd/led_strip/color]
        subscribers: [sensor/ultrasonic/left/distance,sensor/ultrasonic/middle/distance,sensor/ultrasonic/right/distance,sensor/imu/yaw]
        </rosparam>
    </node>

    <!-- <include file="$(find fred_teleop)/launch/fred_teleop.launch" /> -->

    <include file="$(find fred_move_base)/launch/safe_twist.launch" /> 
    <include file="$(find fred_move_base)/launch/odom.launch" />

    <!-- <include file="$(find fred_hardware_interface)/launch/interface.launch" /> -->

    <!-- <include file="$(find fred_rcx)/launch/rcx.launch" /> -->



</launch>